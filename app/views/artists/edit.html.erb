<h1>Artists#edit</h1>
<p>Find me in app/views/artists/edit.html.erb</p>


<% if @artist.errors.any? %>

    <ul>
    <% @artist.errors.full_messages.each do |msg| %>
      <span class="error">*<%= msg %></span>
    <% end %>
    </ul>
<% end %>





<%=form_for @artist do |f| %>

<p>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
</p>

<p>
  <%= f.label :city %><br>
  <%= f.text_field :city %>
</p>

<p>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</p>


<p>
  <%= f.label :instagram %><br>
  <%= f.text_field :instagram %>
</p>

<p>
  <%= f.label :phone_number %><br>
  <%= f.text_field :phone_number %>
</p>







<p>
   <%= f.submit "Save" %>
 </p>

 <% end %>
